<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane hgap="10.0" prefHeight="223.0" prefWidth="316.0" vgap="10.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="184.0" minWidth="10.0" prefWidth="133.0" />
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="215.0" minWidth="10.0" prefWidth="215.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <Label text="Marka" />
        <Label text="Serija" GridPane.rowIndex="1" />
        <Label text="Broj mjesta" GridPane.rowIndex="2" />
        <ButtonBar prefHeight="32.0" prefWidth="148.0" GridPane.columnSpan="2" GridPane.rowIndex="4">
            <buttons>
                <Button fx:id="btnOK" mnemonicParsing="false" onAction="#potvrdi" text="Ok" />
                <Button mnemonicParsing="false" onAction="#zatvori" text="Cancel" />
            </buttons>
            <padding>
                <Insets left="30.0" />
            </padding>
            <GridPane.margin>
                <Insets right="30.0" />
            </GridPane.margin>
        </ButtonBar>
        <TextField fx:id="fldMarka" prefHeight="25.0" prefWidth="244.0" GridPane.columnIndex="1" />
        <TextField fx:id="fldSerija" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <Spinner fx:id="spiner" max="100" min="20" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</GridPane>